# API Configuration
VITE_API_URL=http://localhost:5000

# Service Configuration - Set to 'true' to use real NeoV3 backend services
VITE_USE_REAL_WEBSOCKET=false
VITE_USE_REAL_FILE_SERVICE=false
VITE_USE_REAL_AUTH_SERVICE=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES_PER_UPLOAD=5

# WebSocket Configuration
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5
VITE_WEBSOCKET_RECONNECT_DELAY=1000

# Debug Configuration
VITE_DEBUG_SERVICES=false
VITE_LOG_LEVEL=info

# Authentication Configuration
VITE_TOKEN_STORAGE_KEY=access_token
VITE_REFRESH_TOKEN_STORAGE_KEY=refresh_token
